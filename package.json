{
  "name": "roguelike-js",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'npm run start:dev-server' 'npm run start:electron:dev'",
    "start:dev-server": "NODE_ENV=development webpack-dev-server --hot --inline --config webpack/webpack.client.config.js",
    "start:electron:dev": "NODE_ENV=development webpack --bail --config webpack/webpack.main.config.js && electron ./dist/main.js",
    "build:renderer:dev": "NODE_ENV=development webpack --bail --config webpack/webpack.client.config.js",
    "clean": "rimraf dist"
  },
  "author": "Matt Hink <matt.hink@formidable.com>",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.4.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.11.1",
    "siimple": "^1.3.7"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-dev": "^2.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^3.0.0",
    "css-loader": "^0.25.0",
    "devtron": "^1.3.0",
    "electron-debug": "^1.0.1",
    "electron-devtools-installer": "^2.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
}
