{
  "name": "roguelike-js",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -k -s first 'npm run start:dev-server' 'npm run start:electron:dev'",
    "start:dev-server": "NODE_ENV=development webpack-dev-server --hot --inline --config webpack/webpack.client.config.js",
    "start:electron:dev": "NODE_ENV=development webpack --bail --config webpack/webpack.main.config.js && electron ./dist/main.js",
    "build:renderer:dev": "NODE_ENV=development webpack --bail --config webpack/webpack.client.config.js",
    "clean": "rimraf dist",
    "lint": "eslint --color --ext .js client",
    "flow": "flow",
    "test": "node -r babel-register -r babel-polyfill ./test/index.js"
  },
  "author": "Matt Hink <matt.hink@formidable.com>",
  "license": "MIT",
  "dependencies": {
    "electron": "^1.4.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.11.1",
    "siimple": "^1.3.7",
    "tinyqueue": "^1.1.0",
    "uuid": "^2.0.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-dev": "^2.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^3.0.0",
    "css-loader": "^0.25.0",
    "devtron": "^1.3.0",
    "electron-debug": "^1.0.1",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "^3.7.1",
    "eslint-config-formidable": "^2.0.1",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-filenames": "^1.1.0",
    "eslint-plugin-flowtype": "^2.20.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-redux-saga": "^0.1.4",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.33.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "rimraf": "^2.5.4",
